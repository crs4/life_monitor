{
    "name": "lifemonitor",
    "description": "Workflow Testing Service",
    "version": "0.2.0",
    "license": "MIT",
    "author": "CRS4",
    "main": "../dist/js/lifemonitor.min.js",
    "scripts": {
        "css": "npm-run-all css-compile css-prefix css-minify",
        "css-splits": "npm-run-all css-compile-splits css-prefix-splits css-minify-splits",
        "css-all": "npm-run-all --parallel css css-splits",
        "css-compile-bash": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js --output-style expanded --source-map true --source-map-contents true --precision 6 ",
        "css-compile": "npm run css-compile-bash -- scss/lifemonitor.scss ../dist/css/lifemonitor.css",
        "css-compile-splits": "npm run css-compile-bash -- scss/parts -o ../dist/css/alt/",
        "css-prefix": "postcss --config config/postcss.config.js --replace \"../dist/css/*.css\" \"!../dist/css/*.min.css\"",
        "css-prefix-splits": "postcss --config config/postcss.config.js --replace \"../dist/css/alt/*.css\" \"!../dist/css/alt/*.min.css\"",
        "css-minify-bash": "cleancss -O1 --format breakWith=lf --with-rebase --source-map --source-map-inline-sources --output ",
        "css-minify": "npm run css-minify-bash -- ../dist/css/ --batch --batch-suffix \".min\" \"../dist/css/*.css\" \"!../dist/css/*.min.css\"",
        "css-minify-splits": "npm run css-minify-bash -- ../dist/css/alt/ --batch --batch-suffix \".min\" \"../dist/css/alt/*.css\" \"!../dist/css/alt/*.min.css\"",
        "css-lint": "stylelint \"scss/**/*.scss\" --cache --cache-location .cache/.stylelintcache",
        "compile": "npm-run-all --parallel js css-all",
        "dev": "npm-run-all --parallel",
        "lockfile-lint": "lockfile-lint --allowed-hosts npm --allowed-schemes https: --empty-hostname false --type npm --path package-lock.json",
        "postinstall": "",
        "js": "npm-run-all js-compile js-minify js-plugins",
        "js-plugins": "./config/pkg-plugins.sh",
        "js-compile": "rollup --config config/rollup.config.js --sourcemap",
        "js-minify": "terser --compress typeofs=false --mangle --comments \"/^!/\" --source-map  --output ../dist/js/lifemonitor.min.js ../dist/js/lifemonitor.js",
        "js-lint": "eslint --cache --cache-location .cache/.eslintcache --report-unused-disable-directives .",
        "lint": "npm-run-all --continue-on-error --parallel css-lint js-lint lockfile-lint",
        "production": "npm-run-all --parallel compile",
        "prepare-release": "npm-run-all production",
        "test": "npm-run-all lint production"
    },
    "keywords": [
        "testing",
        "workflow"
    ],
    "homepage": "https://lifemonitor.eu",
    "style": "../dist/css/lifemonitor.css",
    "sass": "scss/lifemonitor.scss",
    "repository": {
        "type": "git",
        "url": "git://github.com/crs4/life_monitor.git"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.15.3",
        "@lgaitan/pace-progress": "^1.0.7",
        "@sweetalert2/theme-bootstrap-4": "^4.0.3",
        "@ttskch/select2-bootstrap4-theme": "^1.5.2",
        "bootstrap": "^4.6.0",
        "bootstrap-colorpicker": "^3.2.0",
        "bootstrap-slider": "^11.0.2",
        "bootstrap-switch": "3.4.0",
        "bootstrap4-duallistbox": "^4.0.2",
        "icheck-bootstrap": "^3.0.1",
        "jquery": "^3.6.0",
        "jqvmap-novulnerability": "^1.5.1",
        "overlayscrollbars": "^1.13.1",
        "summernote": "^0.8.18",
        "sweetalert2": "^10.15.6",
        "tempusdominus-bootstrap-4": "^5.39.0",
        "tempusdominus-core": "^5.19.0",
        "toastr": "^2.1.4",
        "uplot": "^1.6.7"
    },
    "devDependencies": {
        "@rollup/plugin-babel": "^5.3.0",
        "clean-css-cli": "^5.2.2",
        "concurrently": "^6.0.0",
        "copyfiles": "2.4.1",
        "eslint": "^7.22.0",
        "eslint-config-xo": "^0.35.0",
        "eslint-plugin-compat": "^3.9.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-unicorn": "^28.0.2",
        "lockfile-lint": "^4.6.2",
        "node-sass": "^5.0.0",
        "node-sass-package-importer": "^5.3.2",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.2.8",
        "postcss-cli": "^8.3.1",
        "postcss-scrollbar": "^0.3.0",
        "rollup": "^2.42.1",
        "stylelint": "^13.12.0",
        "stylelint-config-twbs-bootstrap": "^2.1.0",
        "terser": "^5.6.1"
    }
}
